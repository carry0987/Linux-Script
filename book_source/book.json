{
    "title": "Linux-Script",
    "author": "carry0987",
    "description": "Store Linux Script",
    "defaultDescription": "Store Linux Script",
    "language": "en",
    "plugins": [
        "github",
        "-lunr",
        "-search",
        "-sharing",
        "-page-toc-button",
        "search-plus",
        "anchor-navigation-ex",
        "flexible-alerts",
        "code",
        "prism",
        "-highlight",
        "tbfed-pagefooter",
        "hide-element",
        "favicon-absolute"
    ],
    "pluginsConfig": {
        "fontSettings": {
            "theme": "night",
            "family": "sans"
        },
        "theme-default": {
            "showLevel": false
        },
        "github": {
            "url": "https://github.com/carry0987/Linux-Script"
        },
        "anchor-navigation-ex": {
            "showLevel": false,
            "associatedWithSummary": true,
            "printLog": false,
            "multipleH1": false,
            "mode": "float",
            "showGoTop": true,
            "float": {
                "floatIcon": "fa fa-navicon",
                "showLevelIcon": false,
                "level1Icon": "fa fa-hand-o-right",
                "level2Icon": "fa fa-hand-o-right",
                "level3Icon": "fa fa-hand-o-right"
            },
            "pageTop": {
                "showLevelIcon": false,
                "level1Icon": "fa fa-hand-o-right",
                "level2Icon": "fa fa-hand-o-right",
                "level3Icon": "fa fa-hand-o-right"
            }
        },
        "flexible-alerts": {
            "style": "flat"
        },
        "code": {
            "copyButtons": true
        },
        "prism": {
            "css": [
                "prismjs/themes/prism-tomorrow.css"
            ],
            "lang": {
                "flow": "typescript"
            },
            "ignore": [
                "eval-js"
            ]
        },
        "tbfed-pagefooter": {
            "copyright": "Copyright &copy carry0987",
            "modify_label": "Last Modified : ",
            "modify_format": "YYYY-MM-DD HH:mm:ss"
        },
        "hide-element": {
            "elements": [".gitbook-link"]
        },
        "favicon-absolute": {
            "favicon": "https://carry0987.github.io/favicon.png"
        }
    }
}
